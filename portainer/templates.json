{
    "version": "3",
    "templates": [
        {
            "type": 1,
            "name": "code-server",
            "title": "VS Code Server",
            "image": "linuxserver/code-server:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "description": "VS Code running on a remote server",
            "network": "host",
            "ports": [
                ":8443/tcp"
            ],
            "volumes": [
                {
                    "container": "config",
                    "bind": "/code-server/config"
                }
            ],
            "env": [
                {
                    "name": "PUID",
                    "label": "PUID",
                    "default": "1000"
                },
                {
                    "name": "GUID",
                    "label": "PUID",
                    "default": "1000"
                },
                {
                    "name": "TZ",
                    "label": "TZ",
                    "default": "Europe/London",
                    "description": "Specify a timezone"
                },
                {
                    "label": "PASSWORD",
                    "name": "PASSWORD"
                },
                {
                    "label": "SUDO_PASSWORD",
                    "name": "SUDO_PASSWORD"
                },
                {
                    "label": "PROXY_DOMAIN",
                    "name": "PROXY_DOMAIN"
                }
            ],
            "id": 1
        },
        {
            "type": 1,
            "name": "homeassistant",
            "title": "Homeassistant",
            "image": "linuxserver/homeassistant:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "description": "Open source home automation that puts local control and privacy first",
            "network": "host",
            "ports": [
                "8123:8123/tcp"
            ],
            "volumes": [
                {
                    "container": "/config",
                    "bind": "/homeassistant/config"
                }
            ],
            "env": [
                {
                    "name": "PUID",
                    "label": "PUID",
                    "default": "1000"
                },
                {
                    "name": "PGID",
                    "label": "PGID",
                    "default": "100"
                },
                {
                    "name": "TZ",
                    "label": "TZ",
                    "default": "Europe/London",
                    "description": "Specify a timezone"
                }
            ],
            "id": 2
        },
        {
            "type": 1,
            "name": "echobase",
            "title": "Echobase",
            "image": "ghcr.io/kammetherell/echobase:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "description": "Homelab controller including managing backups to S3",
            "ports": [
                ":5000/tcp"
            ],
            "volumes": [
                {
                    "container": "/app/db"
                },
                {
                    "container": "/var/lib/docker.sock",
                    "bind": "/var/lib/docker.sock"
                }
            ],
            "id": 3
        },
        {
            "type": 1,
            "name": "nginx-proxy-manager",
            "title": "Nginx Proxy Manager",
            "image": "jc21/nginx-proxy-manager:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "logo": "https://raw.githubusercontent.com/Qballjos/portainer_templates/master/Images/proxy_mgr.png",
            "description": "Nginx Proxy Manager enables you to easily forward to your websites running at home or otherwise, including free SSL",
            "env": [
                {
                    "label": "DB_SQLITE_FILE",
                    "name": "DB_SQLITE_FILE",
                    "default": "/data/database.sqlite"
                }
            ],
            "ports": [
                "80:80/tcp",
                "81:81/tcp",
                "443:443/tcp"
            ],
            "volumes": [
                {
                    "container": "/data"
                },
                {
                    "container": "/etc/letsencrypt"
                }
            ],
            "id": 4
        },
        {
            "type": 1,
            "name": "vaultwarden",
            "title": "Vaultwarden",
            "image": "vaultwarden/server:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "logo": "https://raw.githubusercontent.com/Qballjos/portainer_templates/master/Images/bitwarden.png",
            "description": "Locally hosted Password manager based on Bitwarden",
            "ports": [
                ":80/tcp"
            ],
            "volumes": [
                {
                    "container": "/config"
                }
            ],
            "id": 5
        },
        {
            "type": 1,
            "name": "adguard",
            "title": "Adguard",
            "image": "adguard/adguardhome:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "description": "Adguard Home instance with ports required as plain DNS server and DHCP server if desired",
            "network": "host",
            "ports": [
                ":80/tcp",
                "3000:3000/tcp",
                "53:53/tcp",
                "53:53/udp",
                "67:67/udp",
                "68:68/tcp",
                "68:68/udp"
            ],
            "volumes": [
                {
                    "container": "/opt/adguardhome/conf",
                    "bind": "/adguard/config"
                },
                {
                    "container": "/opt/adguardhome/work",
                    "bind": "/adguard/work"
                }
            ],
            "id": 6
        },
        {
            "type": 1,
            "name": "jellyfin",
            "title": "Jellyfin",
            "image": "linuxserver/jellyfin:latest",
            "platform": "linux",
            "restart_policy": "unless-stopped",
            "description": "Free Software Media System",
            "network": "host",
            "ports": [
                ":8096/tcp",
                ":8920/tcp"
            ],
            "volumes": [
                {
                    "container": "config",
                    "bind": "/jellyfin/config"
                },
                {
                    "container": "/data/movies",
                    "bind": "/media/movies"
                }
            ],
            "env": [
                {
                    "name": "PUID",
                    "label": "PUID",
                    "default": "1000"
                },
                {
                    "name": "PGID",
                    "label": "PGID",
                    "default": "1000"
                },
                {
                    "name": "TZ",
                    "label": "TZ",
                    "default": "Europe/London",
                    "description": "Specify a timezone"
                },
                {
                    "label": "PASSWORD",
                    "name": "PASSWORD"
                },
                {
                    "label": "SUDO_PASSWORD",
                    "name": "SUDO_PASSWORD"
                },
                {
                    "label": "PROXY_DOMAIN",
                    "name": "PROXY_DOMAIN"
                }
            ],
            "id": 7
        }
    ]
}